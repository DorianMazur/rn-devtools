{
  "name": "@rn-devtools/react-navigation-plugin",
  "author": "Dorian Mazur",
  "license": "MIT",
  "version": "0.0.1",
  "description": "React Navigation tab for rn-devtools",
  "main": "dist/web.js",
  "types": "dist/web.d.ts",
  "exports": {
    ".": {
      "import": "./dist/web.js",
      "types": "./dist/web.d.ts"
    },
    "./web": {
      "import": "./dist/web.js",
      "types": "./dist/web.d.ts"
    },
    "./native": {
      "import": "./dist/native.js",
      "types": "./dist/native.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "build": "tsup src/web.tsx src/native.ts --format esm --dts --clean --external react --external react-dom --external socket.io-client"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.17",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@react-navigation/devtools": "^7.0.38",
    "@rn-devtools/plugin-sdk": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.4.0",
    "eslint-plugin-react": "7.37.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "socket.io-client": "4"
  }
}
