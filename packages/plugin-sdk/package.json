{
  "name": "@rn-devtools/plugin-sdk",
  "author": "Dorian Mazur",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Plugin SDK for rn-devtools",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "build": "tsup src/index.ts --format esm --dts --clean --external react --external react-dom --external socket.io-client"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  },
  "peerDependencies": {
    "socket.io-client": "4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.4.0",
    "eslint-plugin-react": "7.37.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
