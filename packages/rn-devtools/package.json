{
  "name": "rn-devtools",
  "version": "0.0.1",
  "description": "React Native DevTools",
  "type": "module",
  "main": "./nativeDist/index.js",
  "types": "./nativeDist/index.d.ts",
  "exports": {
    ".": {
      "import": "./nativeDist/index.js",
      "types": "./nativeDist/index.d.ts"
    }
  },
  "scripts": {
    "build:web": "vite build",
    "build:cli": "tsup cli/index.ts --format esm --platform=node",
    "build:native": "tsup ./native/index.ts --format esm --dts --clean --external socket.io-client --out-dir nativeDist",
    "run:cli": "node ./bin/index.js",
    "run:web": "vite preview",
    "dev:web": "vite dev"
  },
  "bin": "./bin/index.js",
  "dependencies": {
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "isbot": "^5.1.27",
    "open": "^10.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "^7.7.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "4.8.1",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "tailwindcss": "^4.1.4",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
