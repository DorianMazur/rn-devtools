{
  "name": "@rn-devtools/react-query-plugin",
  "author": "Dorian Mazur",
  "license": "MIT",
  "version": "0.0.1",
  "description": "React Query tab for rn-devtools",
  "main": "dist/web.mjs",
  "types": "dist/web.d.mts",
  "exports": {
    ".": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./native": {
      "import": "./dist/native.mjs",
      "types": "./dist/native.d.mts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "build": "tsup src/web.tsx src/native.ts --format esm --dts --clean --external react --external react-dom --external socket.io-client"
  },
  "dependencies": {
    "@rn-devtools/plugin-sdk": "workspace:*",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "7.37.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "socket.io-client": "4"
  }
}
