{
  "name": "@rn-devtools/react-query-plugin",
  "author": {
    "name": "Dorian Mazur",
    "email": "mazur.dorian15@gmail.com"
  },
  "homepage": "https://github.com/DorianMazur/rn-devtools",
  "repository": {
    "type": "git",
    "url": "git://github.com/DorianMazur/rn-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/DorianMazur/rn-devtools/issues"
  },
  "license": "MIT",
  "version": "1.0.0",
  "description": "React Query tab for rn-devtools",
  "main": "dist/web.mjs",
  "types": "dist/web.d.mts",
  "exports": {
    ".": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./native": {
      "import": "./dist/native.mjs",
      "types": "./dist/native.d.mts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "build": "tsup src/web.tsx src/native.ts --format esm --dts --clean --external react --external socket.io-client",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@rn-devtools/plugin-sdk": "^1.0.2",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "react": "19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "7.37.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@rn-devtools/plugin-sdk": "1",
    "react": ">=18"
  }
}
