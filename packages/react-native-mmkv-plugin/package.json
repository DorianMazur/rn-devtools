{
  "name": "@rn-devtools/react-native-mmkv-plugin",
  "author": "Dorian Mazur",
  "license": "MIT",
  "version": "0.0.1",
  "description": "React Native MMKV plugin for rn-devtools",
  "main": "dist/web.mjs",
  "types": "dist/web.d.mts",
  "exports": {
    ".": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "types": "./dist/web.d.mts"
    },
    "./native": {
      "import": "./dist/native.mjs",
      "types": "./dist/native.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "build": "tsup src/web.tsx src/native.ts --format esm --dts --clean --external react --external react-dom --external socket.io-client"
  },
  "dependencies": {
    "@rn-devtools/plugin-sdk": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-mmkv": "patch:react-native-mmkv@npm%3A3.3.0#~/.yarn/patches/react-native-mmkv-npm-3.3.0-27a8e718b6.patch",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.4.0",
    "eslint-plugin-react": "7.37.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-native-mmkv": "3",
    "socket.io-client": "4"
  }
}
